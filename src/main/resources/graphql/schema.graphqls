type Query {
    unit(id: ID): UnitBoundary
    employee(email: ID): EmployeeBoundary
    allUnits(size: Int, page: Int): [UnitBoundary]
}

type UnitBoundary {
    id: ID
    type: String
    manager: Manager
    employees: [EmployeeBoundary]
    creationDate: String
    parentUnit: UnitBoundary
    subUnits(size: Int, page: Int): [UnitBoundary]

}

type EmployeeBoundary {
    email: String
    units: [UnitBoundary]
    manages: [UnitBoundary]

}

type Manager {
    email: String
}





