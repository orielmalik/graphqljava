type Query {
    unit(id: ID): UnitBoundary
    employee(email: String): EmployeeBoundary
    allUnits(size: Int, page: Int): [UnitBoundary]
}

type UnitBoundary {
    id: ID
    type: String
    manager: Manager
    employees: [EmployeeBoundary]
    creationDate: String
    parentUnit: UnitBoundary
    subUnits(size: Int, page: Int): [UnitBoundary]

}

type EmployeeBoundary {
    email: String
    Units: [UnitBoundary]
    manager: UnitBoundary

}

type Manager {
    email: String
}





